generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// ================================================
// Enum
// ================================================

enum Level {
  N1
  N2
  N3
  N4
  N5
}

// ================================================
// Model
// ================================================

model Word{
  id Int @id @default(autoincrement())
  word String @unique
  kana String
  meaning String 
  example String 
  exampleKana String 
  exampleMeaning String 
  level Level
}
